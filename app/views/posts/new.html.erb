<%= form_with(model: @post, local: true) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title%>

    <%= f.label :discription %>
    <%= f.text_area :discription %>

    <%= f.label :github_url %>
    <%= f.url_field :github_url %>

    <%= f.label :app_url %>
    <%= f.url_field :app_url%>

    <%= f.submit '投稿'%>
<% end %>