<div class="card mb-5 back-color">
    <div class="card-img-top">
        <% if post.images.attached? %>
            <% image = post.images[0] %>
            <%= image_tag image, width: '100%', height: '100%' %>
        <% end %>
    </div>ã€€
    <%= link_to post_path(post), class: "link_to_color" do %>
        <div class="card-body">
            <%= image_tag post.user.image, size: '40x40', class: 'rounded_circle', class: "mr-2 card-title" %>
            <%= post.user.name %>
            <h5 class="card-subtitle mr-60-ml-60 mt-2"><%= post.title %></h5>
            <div class="mt-2 mr-60-ml-60">
                <%= string_extraction_limit(post.discription, 200 )%>
            </div>
        </div>
    <% end %>
</div>