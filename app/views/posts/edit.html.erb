<%= form_with(model: @post, local: true) do |f| %>
    <%= render "shared/error_messages", object: f.object%>
    <%= f.label :title %>
    <%= f.text_field :title%>

    <%= f.select :category_id, Category.all.map { |o| [o.name, o.id]}%>

    <%= f.label :discription %>
    <%= f.text_area :discription %>

    <%= f.label :images, 'イメージ画像' %>
    <%= f.file_field :images, multiple: true %>

    <%= f.label :github_url %>
    <%= f.url_field :github_url %>

    <%= f.label :app_url %>
    <%= f.url_field :app_url%>

    <%= f.submit '更新'%>
<% end %>